<plural($n) {
    $n == 0 ? "zero" :
    $n == 1 ? "one"  :
    $n < 10 ? "few"  :
              "many"
}>

<objectsWithCount[plural($objectsNum)] {
    zero: "{{setObjectsNumber}}: 没有对象",
    one: "{{setObjectsNumber}}: 1 个对象",
    few: "{{setObjectsNumber}}: {{ $objectsNum }} 个, 很少对象",
    many: "{{setObjectsNumber}}: {{ $objectsNum }} 个, 很多对象"
}>

<setObjectsNumber "设置对象数量">

